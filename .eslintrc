{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": "off",
        "indent": ["error", 4],
        "operator-linebreak": "off",
        "max-len": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}